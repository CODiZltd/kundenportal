       <record model="ir.ui.view" id="auftraege_search_view">
            <field name="name">auftraege.search</field>
            <field name="model">backend.auftraege</field>
            <field name="arch" type="xml">
                <search>
                    <field name="auf_name"/>
                    <field name="auf_paket"/>
                    <field name="auf_start"/>
                    <field name="auf_ende"/>
